  <style>
    .transform {
      border-radius: 25px;
      background: black;
      padding: 20px;
      width: 80px;
      height: 0.05px;
      transition: width 2s;
      -webkit-transition: width 2s; /* Safari 3.1 to 6.0 */
    }

    .transform:hover {
      width: 150px;
    }
  </style>
