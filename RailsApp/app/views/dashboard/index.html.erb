<body ng-app="myApp" style="background-color: white ;">
<div class="slds-grid" ng-controller="EventCtrl">
  <header class="slds-global-header_container">
    <a href="javascript:void(0);" class="slds-assistive-text slds-assistive-text--focus">Skip to Navigation</a>
    <a href="javascript:void(0);" class="slds-assistive-text slds-assistive-text--focus">Skip to Main Content</a>
    <div class="slds-global-header slds-grid slds-grid--align-spread">
      <div class="slds-media slds-no-space slds-grow slds-media_center">
        <div class="slds-media__figure slds-size--0.5-of-1 slds-m-left--xx-large">
          <img src="<%= image_path "images/QA.png" %>" alt="person name" style="height: 70%; width:60%"/>
        </div>
      </div>
      <ul class="slds-global-header__item slds-grid slds-grid--vertical-align-center">
        <div class="slds-text-title_caps"><h1><%= session[:userProfile]["display_name"] %></h1></div>
        <button class="slds-button slds-m-left--small" title="<%= session[:email_address] %>" aria-haspopup="true" ng-click="action = true;" tabindex="1" ng-blur="hidePopup();">
          <span class="slds-avatar slds-avatar--circle slds-avatar--medium">
            <img src="<%= session[:userProfile]["image"]["url"] %>" alt="<%= session[:email_address] %>"/>
          </span>
          <!--          <span class="slds-icon_container slds-icon-standard-user">
                                        <svg aria-hidden="true" class="slds-icon slds-icon--medium">
                                          <use xlink:href="/assets/icons/utility-sprite/svg/symbols.svg#user"></use>
                                        </svg>
                                        <span class="slds-assistive-text">Case icon</span>
                                      </span>-->
          <!--                  <span class="slds-avatar slds-avatar--circle slds-avatar--medium">
                              <abbr class="slds-avatar__initials slds-icon-standard-user" title="person name"><p style="color: white"><%#= session[:email_address].to_s.upcase_first[0] %></p></abbr>
                            </span>-->
        </button>
      </ul>
    </div>
    <div class="slds-popover slds-nubbin--top-right" role="dialog" id="popup" ng-if="action" style="margin-left: 75%" tabindex="1">
      <div class="slds-popover__body slds-grid">
        <div class="slds-col">
          <img src="<%= session[:userProfile]["image"]["url"].gsub('50','100') %>">
        </div>
        <div class="slds-col">
          <div class="slds-text-heading--small slds-truncate slds-align-middle slds-m-left--medium" title="<%= session[:userProfile]["display_name"] %>">
            <b><%= session[:userProfile]["display_name"] %></b></div>
          <ul>
            <li>
              <p class="slds-text-body_small slds-line-height_reset slds-m-top--xx-small"><%= session[:email_address] %></p>
              <span class="slds-float_right ">
                <a href="/logout" tabindex="-1">LogOut</a>
              </span>
            </li>
          </ul>
        </div>
      </div>
    </div>
  </header>
  <div class="page-wrap">
    <!-- Nav -->
    <nav id="nav" style="">
      <ul style="top: 18%">
        <li ng-if="<%= session[:user][0]['admin'] %>">
          <a href="#!users_view" class="slds-size--1-of-1 ng-class: isActive" title="Users" ng-model="isActive" ng-focus="isActive = 'active'" ng-blur="isActive = ''" ng-mouseover="isActive = 'active'" ng-mouseout="isActive = ''">
            <i class="fas fa-users"></i></a></li>
        <li ng-repeat="item in listItems">
          <a href="{{item.url}}" title="{{item.label}}" class="ng-class: item.isActive" ng-focus="item.isActive = 'active'" ng-blur="item.isActive = ''"><i class="ng-class: item.icon"></i></a>
        </li>
      </ul>
    </nav>
